    718          /*********************************************************************
    719           * @fn      GenericApp_ProcessRtosMessage
    720           *
    721           * @brief   Receive message from RTOS queue, send response back.
    722           *
    723           * @param   none
    724           *
    725           * @return  none
    726           */
    727          static void GenericApp_ProcessRtosMessage( void )
    728          {
    729            osalQueue_t inMsg;
    730          
    731            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    732            {
    733              uint8 cmndId = inMsg.cmnd;
    734              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    735          
    736              switch ( cmndId )
    737              {
    738                case CMD_INCR:
    739                  counter += 1;  /* Increment the incoming counter */
    740                                 /* Intentionally fall through next case */
    741          
    742                case CMD_ECHO:
    743                {
    744                  userQueue_t outMsg;
    745          
    746                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    747                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    748                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    749                  break;
    750                }
    751                
    752                default:
    753                  break;  /* Ignore unknown command */    
    754              }
    755            }
    756          }
