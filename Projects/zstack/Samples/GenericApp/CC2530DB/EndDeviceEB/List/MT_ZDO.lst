   2033          void* MT_ZdoSrcRtgCB( void *pStr )
   2034          {
   2035            uint8 len, *pBuf;
   2036            zdoSrcRtg_t *pSrcRtg = pStr;
   2037          
   2038            // srcAddr (2) + relayCnt (1) + relayList( relaycnt * 2 )
   2039            len = 2 + 1 + pSrcRtg->relayCnt * sizeof(uint16);
   2040          
   2041            if (NULL != (pBuf = (uint8 *)osal_mem_alloc(len)))
